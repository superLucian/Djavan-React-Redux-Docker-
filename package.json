{
  "name": "rhino_djavan",
  "version": "1.0.0",
  "description": "Rhino Djavan",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.config",
    "dev": "concurrently --kill-others \"npm run dev-server\" \"npm run webpack-dev-server\"",
    "dev-server": "NODE_ENV=development nodemon --exec babel-node ./bin/server.js --watch ./bin/server.js",
    "webpack-dev-server": "NODE_ENV=development webpack-dev-server --config webpack.config",
    "start": "concurrently --kill-others \"npm run start-server\"",
    "start-server": "NODE_ENV=production babel-node ./bin/server.js",
    "lint": "eslint 'src/**/*.js' 'webpack.config/*.js' 'bin/*.js'",
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text mocha mochaSetup.js './test/**/**/*.test.@(js|jsx)' --compilers js:babel-register --require babel-polyfill --recursive",
    "test": "NODE_ENV=test mocha mochaSetup.js './test/**/**/*.test.@(js|jsx)' --compilers js:babel-register --require babel-polyfill --recursive",
    "debug": "NODE_ENV=test mocha debug mochaSetup.js './test/**/**/*.test.@(js|jsx)' --compilers js:babel-register --require babel-polyfill --recursive",
    "test:watch": "npm test -- --watch",
    "ci-test": "npm run lint",
    "commit": "git-cz"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "docs",
        "ci"
      ],
      "maxSubjectLength": 150
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "keywords": [
    "React",
    "Redux",
    "Isomorphic"
  ],
  "author": "Art Richards (art@trucktech.in)",
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
